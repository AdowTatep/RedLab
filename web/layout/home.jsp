<section class="row">
    <div class="loginForm col s12 m5 content-center ">
        <div class="card">
            <div class="card-content">
                
                <div class="content-center msgs">
                    <div class="topMsg">
                        <span class="wlcMessage">Faça seu login</span>
                    </div>
                </div>
                
                <form method="POST" action="#" class="">
                    
                    
                    <div class="row">
                        <div class="input-field col s12">
                            
                            <input  id="loginCampo" type="text" class="validate" name="login">
                            
                            <label for="loginCampo">Login</label>
                            
                        </div>
                    </div>
                    
                    <div class="row">
                        <div class="input-field col s12">
                            
                            <input  id="senhaCampo" type="password" class="validate" name="senha">
                            
                            <label for="senhaCampo">Senha</label>
                            
                        </div>
                    </div>
                    
                </form>
            </div>
            
            
            <div class="card-action">
                <button class="btn waves-effect red darken-4" type="submit" name="action">
                    Fazer Login
                    <i class="material-icons right">send</i>
                </button>
            </div>
        </div>
    </div>
</section>